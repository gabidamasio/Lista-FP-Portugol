programa {
  funcao inicio() {
    real alt, larg, total

    escreva("Insira a Altura do campo: ")
    leia(alt)
    escreva("\n Insira a Largura dpo campo: ")
    leia(larg)

    total = (alt * larg)/5

    escreva("\n A quantidade necessária de grama para cobrir esse campo será de: ", total, " m²")

  }
}
